<%- model_class = Session -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:dateservice) %>:</strong></dt>
  <dd><%= @session.dateservice %></dd>
  <dt><strong><%= model_class.human_attribute_name(:copayowed) %>:</strong></dt>
  <dd><%= @session.copayowed %></dd>
  <dt><strong><%= model_class.human_attribute_name(:copayd) %>:</strong></dt>
  <dd><%= @session.copayd %></dd>
  <dt><strong><%= model_class.human_attribute_name(:claimsubmit) %>:</strong></dt>
  <dd><%= @session.claimsubmit %></dd>
  <dt><strong><%= model_class.human_attribute_name(:claimrecvd) %>:</strong></dt>
  <dd><%= @session.claimrecvd %></dd>
  <dt><strong><%= model_class.human_attribute_name(:claimowed) %>:</strong></dt>
  <dd><%= @session.claimowed %></dd>
  <dt><strong><%= model_class.human_attribute_name(:claimpd) %>:</strong></dt>
  <dd><%= @session.claimpd %></dd>
  <dt><strong><%= model_class.human_attribute_name(:cptcode) %>:</strong></dt>
  <dd><%= @session.cptcode %></dd>
  <dt><strong><%= model_class.human_attribute_name(:sessionnotes) %>:</strong></dt>
  <dd><%= @session.sessionnotes %></dd>
  <dt><strong><%= model_class.human_attribute_name(:client_id) %>:</strong></dt>
  <dd><%= @session.client_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              sessions_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_session_path(@session), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              session_path(@session),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
